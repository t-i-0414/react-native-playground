{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "allowJs": false
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    ".storybook/**/*",
    "app.config.ts",
    "jest.config.ts",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
